{
  "name": "eslint-config-trendmicro",
  "version": "4.0.0",
  "description": "Trend Micro's ESLint config.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "lint": "eslint .",
    "prepublish": "npm run build && (in-install || true) && (not-in-publish || npm test) && safe-publish-latest",
    "pretest": "npm run --silent lint",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trendmicro-frontend/eslint-config-trendmicro"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "trendmicro",
    "javascript",
    "styleguide"
  ],
  "author": "Cheton Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trendmicro-frontend/eslint-config-trendmicro/issues"
  },
  "homepage": "https://github.com/trendmicro-frontend/eslint-config-trendmicro",
  "dependencies": {
    "@eslint/js": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "in-publish": "^2.0.1",
    "rollup": "^4.0.0",
    "safe-publish-latest": "^2.0.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest"
  },
  "engines": {
    "node": ">=20"
  }
}
